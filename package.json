{
	"name": "@tecsense-limited/serenity-cms",
	"description": "CMS developed utilizing Laminas MVC as the backbone.",
	"version": "0.0.0-development",
	"author": "Steve Moore (https://twitter.com/RealMythotical)",
	"bugs": {
		"url": "https://github.com/Tecsense-Limited/Serenity-CMS/issues"
	},
	"scripts": {
		"commit": "git-cz",
		"test": "xo"
	},
	"dependencies": {
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/commit-analyzer": "^8.0.1",
		"@semantic-release/git": "^9.0.0",
		"@semantic-release/github": "^7.2.1",
		"@semantic-release/gitlab": "github:semantic-release/gitlab",
		"@semantic-release/npm": "^7.1.0",
		"@semantic-release/release-notes-generator": "^9.0.2"
	},
	"devDependencies": {
		"commitizen": "^4.0.3",
		"cz-conventional-changelog": "^3.0.2",
		"xo": "^0.38.2"
	},
	"engines": {
		"node": ">=10.18"
	},
	"homepage": "https://github.com/Tecsense-Limited/Serenity-CMS#readme",
	"keywords": [
		"git",
		"cupara",
		"cupara github",
		"github",
		"issue",
		"notifications",
		"publish",
		"pull-request",
		"release",
		"version"
	],
	"license": "BSD-3-Clause",
	"main": "README.md",
	"files": [
		"README.md"
	],
	"prettier": {
		"printWidth": 120,
		"trailingComma": "es5"
	},
	"publicConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Tecsense-Limited/Serenity-CMS.git"
	},
	"xo": {
		"prettier": true,
		"space": true,
		"rules": {
			"camelcase": [
				"error",
				{
					"properties": "never"
				}
			],
			"unicorn/string-content": "off",
			"unicorn/no-reduce": "off"
		}
	},
	"renovate": {
		"extends": [
			"github>tecsense-limited/serenity-cms/.github"
		]
	}
}
